{
    "version": "0.1",
    "name": "aiida-lsmo-workflows",
    "author": "Aliaksandr Yakutovich, Daniele Ongari, Pezhman Zarabadi-Poor",
    "author_email": "aliaksandr.yakutovich@epfl.ch",
    "description": "AiiDA workflows for the LSMO laboratory at EPFL",
    "url": "https://github.com/yakutovicha/aiida-lsmo-workflows",
    "license": "MIT License",
    "classifiers": [
        "Programming Language :: Python"
    ],
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida-core == 1.0.0b5"
    ],
    "entry_points": {
        "aiida.workflows": [
            "aiida_lsmo_workflows.isotherm_multi_comp_serial=aiida_lsmo_workflows.isotherm_multi_comp_serial:MultiCompIsothermWorkChain",
            "aiida_lsmo_workflows.gemc=aiida_lsmo_workflows.gemc:GEMCWorkChain"
            ]
    }
}
